        "HosipitalEntry": {
            "properties": {
                "date": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "diagnosisCodes": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "discharge": {
                    "$ref": "#/definitions/Discharge"
