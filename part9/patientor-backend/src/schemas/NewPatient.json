                "date",
                "description",
                "discharge",
                "id",
                "specialist",
                "type"
            ],
            "type": "object"
        },
        "OccupationalHealthcareEntry": {
            "properties": {
                "date": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "diagnosisCodes": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
